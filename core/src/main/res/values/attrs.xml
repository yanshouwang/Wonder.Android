<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="curveNavigationStyle" format="reference" />

    <declare-styleable name="CurveNavigationView">
        <attr name="backgroundTint" />
        <attr name="menu" />
        <attr name="iconSize" />
        <attr name="iconTint" format="color" />
        <attr name="elevation" />
    </declare-styleable>

    <declare-styleable name="Indicator">
        <attr name="count" format="integer" />
        <attr name="dotSize" format="dimension" />
        <attr name="dotSpacing" format="dimension" />
        <attr name="dotExtension" format="dimension" />
        <attr name="inactiveColor" format="color" />
        <attr name="activeColor" format="color" />
    </declare-styleable>
</resources>